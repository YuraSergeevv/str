        <script src="https://cdn.jsdelivr.net/npm/chart.js@3.4.1/dist/chart.min.js"></script>
        <script>

            pickmeup('.date_start', {
                position: 'bottom',
                hide_on_select: true,
                locale: 'ru',
            })



            pickmeup('.date_end', {
                position: 'bottom',
                hide_on_select: true,
                locale: 'ru',
            })


            let labels = ['01.06', '02.06', '03.06', '04.06', '05.06', '06.06', '07.06', '08.06'];

            let data = [
                {
                    label: 'Dataset 1',
                    data: [34, 38, 24, 49, 36, 13, 30, 24],
                    borderColor: '#51c0bf',
                    backgroundColor: '#51c0bf',
                },
                {
                    label: 'Dataset 2',
                    data: [7, 5, 36, 40, 13, 18, 12, 13],
                    borderColor: '#fecc60',
                    backgroundColor: '#fecc60',
                },
                {
                    label: 'Dataset 3',
                    data: [32, 18, 40, 7, 3, 47, 26, 8],
                    borderColor: '#3da3e8',
                    backgroundColor: '#3da3e8',
                },
                {
                    label: 'Dataset 4',
                    data: [46, 44, 15, 38, 8, 27, 10, 37],
                    borderColor: '#76a3e8',
                    backgroundColor: '#76a3e8',
                },
            ];

            let max = 0;
            for (let i = 0; i < data.length; i++) {
                for (let j = 0; j < data[i].data.length; j++) {
                    if (data[i].data[j] > max) {
                        max = data[i].data[j];
                    }
                }
            }

            max = Math.ceil((max / 10)) * 10;

            Chart.defaults.font.size = 10;
            Chart.defaults.color = "#cecece"
            var ctx = document.getElementById('chart');
            var myChart = new Chart(ctx, {
                type: 'line',
                data: {
                    labels: labels,
                    datasets: data,
                },
                options: {
                    scales: {
                        y: {
                            beginAtZero: true,
                            position: 'left',
                            min: 0,
                            max: max,
                        },
                        t: {
                            beginAtZero: true,
                            position: 'right',
                            min: 0,
                            max: max,
                            ticks: {
                                callback: function (value, index, values) {
                                    return '$' + value;
                                },
                            },

                        },
                    },
                    plugins: {
                        legend: {
                            display: false,
                        }
                    }
                }
            });
        </script>

